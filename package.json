    "prettier-plugin-tailwindcss": "^0.6.11",
    "svelte": "^5.20.1",
    "svelte-check": "^4.1.4",
    "svelte-sonner": "^0.3.28",
    "tailwind-merge": "^2.6.0",
    "tailwind-variants": "^0.3.1",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.7.3",
    "unplugin-turbo-console": "^1.11.2",
    "vite": "^6.1.0"
